{
  "name": "flaredrive",
  "version": "0.0.1",
  "description": "CloudFlare R2 storage manager with Pages and Workers.",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "postinstall": "nuxt prepare",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:push": "wrangler d1 migrations apply DB --local",
    "drizzle:push-prod": "wrangler d1 migrations apply DB --remote",
    "drizzle:studio": "cross-env DRIZZLE_STUDIO_MODE=local drizzle-kit studio",
    "drizzle:studio-prod": "cross-env DRIZZLE_STUDIO_MODE=prod drizzle-kit studio",
    "deploy": "wrangler deploy",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,vue,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-epb/flaredrive-rev.git"
  },
  "author": "dragon-fish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/project-epb/flaredrive-rev/issues"
  },
  "homepage": "https://github.com/project-epb/flaredrive-rev#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.970.0",
    "@aws-sdk/s3-request-presigner": "^3.970.0",
    "@tabler/icons-vue": "^3.36.1",
    "@vueuse/core": "^14.1.0",
    "@vueuse/nuxt": "14.1.0",
    "axios": "^1.13.2",
    "cloudflare": "^5.2.0",
    "drizzle-orm": "^0.45.1",
    "fexios": "^6.1.1",
    "highlight.js": "^11.11.1",
    "naive-ui": "^2.43.2",
    "nuxt": "^4.2.2",
    "p-queue": "^9.1.0",
    "pinia": "^3.0.4",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "vfonts": "^0.0.3",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "vue-waterfall-plugin-next": "^3.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260116.0",
    "@iconify-json/lucide": "^1.2.85",
    "@nuxt/icon": "^2.2.0",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.3",
    "@prettier/plugin-pug": "^3.4.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.9",
    "@unocss/nuxt": "^66.6.0",
    "@vue/language-plugin-pug": "^3.2.2",
    "autoprefixer": "^10.4.23",
    "better-sqlite3": "^12.6.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "nuxtjs-naive-ui": "^1.0.2",
    "prettier": "^3.8.0",
    "pug": "^3.0.3",
    "sass-embedded": "^1.97.2",
    "tailwindcss": "^4.1.18",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
